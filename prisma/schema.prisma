generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider  = "mysql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model TalentRegistration {
  id            String   @id @default(cuid())
  firstName     String
  lastName      String
  email         String
  phone         String
  age           Int
  gender        String
  height        Int
  weight        Int
  eyeColor      String
  hairColor     String
  languages     String
  experience    String
  agency        String
  agencyContact String
  specialSkills String
  availability  String
  location      String
  bio           String
  photos        String   @db.Text
  video         String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
